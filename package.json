{
  "name": "thingview",
  "version": "0.4-alpha",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build && cp src/server/server.js dist/",
    "serve": "vite preview",
    "clean": "rm node_modules/.vite/*",
    "//dist": "For distribution as a self-contained binary",
    "dist": "pkg -c src/server/config.json dist/server.js -o dist/thingview",
    "//install": "Usually wosthub runs as a local user",
    "install": "cp dist/thingview ~/bin/wosthub/bin"
  },
  "bin": "src/server/server.js",
  "pkg": {
    "assets": [
      "dist/**/*"
    ],
    "targets": [
      "linux"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@quasar/extras": "^1.12.1",
    "@types/lodash-es": "^4.17.5",
    "@vueuse/core": "^7.4.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.3",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "luxon": "^2.3.2",
    "mqtt": "^4.2.8",
    "nanoid": "^3.1.30",
    "quasar": "^2.3.3",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12",
    "vue3-grid-layout": "^1.0.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.0.3",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@types/luxon": "^2.3.1",
    "@types/node": "^16.11.10",
    "@vitejs/plugin-vue": "^1.10.1",
    "pkg": "^5.5.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "typescript": "^4.4.3",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vue-tsc": "^0.29.6"
  },
  "readme": "ERROR: No README data found!",
  "_id": "thingview"
}
